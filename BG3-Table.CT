  <LuaScript>local al = getAddressList()
EnableAutoScriptDisabler = al.getMemoryRecordByID(4780)
KeepReattaching = al.getMemoryRecordByID(5519)


function onMemRecPostExecute(memrec, newState, succeeded)
  -- Disable option of auto disabling completly
  if EnableAutoScriptDisabler.Active == true then
    -- Type 11 is a script, At Post execution enabled field will always be True
    if memrec.Type == 11 and memrec.Active == true then
     -- Disable Auto Disabler for [2WS] flag in description
     if(memrec.Description:find("2WS")) then
      print("Activated: " .. memrec.Description .. " [ID:" .. memrec.ID .. "] (Auto Disabler:OFF).")
      return
     end
     memrec.Active = false
     print("Activated: " .. memrec.Description .. " [ID:" .. memrec.ID .. "] (Auto Disabler:ON).")
    end
  end
end


PROCESS_NAME_DX11 = 'bg3_dx11.exe'
PROCESS_NAME_VULCAN = 'bg3.exe'
--------
-------- Auto Attach
--------
local autoAttachTimer = nil ---- variable to hold timer object
local autoAttachTimerInterval = 1000 ---- Timer intervals are in milliseconds
local autoAttachTimerTicks = 0 ---- variable to count number of times the timer has run
local autoAttachTimerTickMax = 5000 ---- Set to zero to disable ticks max
isGameRunning = false
local function autoAttachTimer_tick(timer) ---- Timer tick call back
        ---- Destroy timer if max ticks is reached
	if autoAttachTimerTickMax &gt; 0 and autoAttachTimerTicks &gt;= autoAttachTimerTickMax then
      if KeepReattaching.Active == false then
        print("Auto Attaching disabled TimeOut !")
        timer.destroy()
      end
	end
    if isGameRunning == true then
	 if getProcessIDFromProcessName(PROCESS_NAME_DX11) == nil and getProcessIDFromProcessName(PROCESS_NAME_VULCAN) == nil then
        print("Game has been closed. Waiting on new to be started...")
        isGameRunning = false
     end
    end
    if isGameRunning == false then    ---- Check if process is running
	  if getProcessIDFromProcessName(PROCESS_NAME_DX11) ~= nil then
          isGameRunning = true
          openProcess(PROCESS_NAME_DX11) ---- Open the process
          print("Auto Attached To " .. PROCESS_NAME_DX11 .. "!")

          if KeepReattaching.Active == false then
	       timer.destroy()
          end
      elseif getProcessIDFromProcessName(PROCESS_NAME_VULCAN) ~= nil then
          isGameRunning = true
          openProcess(PROCESS_NAME_VULCAN) ---- Open the process
          print("Auto Attached To " .. PROCESS_NAME_VULCAN .. "!")
          if KeepReattaching.Active == false then
	       timer.destroy()
          end
	  end
    end

	autoAttachTimerTicks = autoAttachTimerTicks + 1 ---- Increase ticks
end
autoAttachTimer = createTimer(getMainForm()) ---- Create timer with the main form as it's parent
autoAttachTimer.Interval = autoAttachTimerInterval ---- Set timer interval
autoAttachTimer.OnTimer = autoAttachTimer_tick ---- Set timer tick call back


